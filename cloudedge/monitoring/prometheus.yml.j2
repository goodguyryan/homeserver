global:
  scrape_interval: 30s
  external_labels:
    monitor: "codelab-monitor"

scrape_configs:
  - job_name: "prometheus"
    scrape_interval: 30s
    static_configs:
      - targets: ["localhost:9090"]
      
  - job_name: "node-home"
    scrape_interval: 30s
    static_configs:
      - targets: ["fedora:9100"]
